        <Menu DockPanel.Dock="Top">
            <MenuItem Header="_File">
                <MenuItem Header="_Open" Click="open_Click"/>
                <MenuItem Header="_Exit" Click="exit_Click"/>
            </MenuItem>
        </Menu>
        <TreeView Name="treeView" DockPanel.Dock="Left">
            <TreeView>
                
            </TreeView>
        </TreeView>
    </DockPanel>
    
</Window>
